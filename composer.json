{
  "name": "holysword/holysword",
  "description": "PHP API framework",
  "type": "project",
  "keywords": [
    "holysword",
    "ziyueyijun",
    "api",
    "framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "ziyueyijun",
      "email": "ziyueyijun@outlook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "monolog/monolog": "^2.0",
    "psr/container": "^1.1",
    "psr/log": "^1.1",
    "psr/http-message": "^1.0",
    "vlucas/phpdotenv": "5.*",
    "symfony/var-dumper": "7.*",
    "filp/whoops": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "HolySword\\": "holysword/"
    },
    "files": [
      "function/function.php",
      "holysword/error.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}